[init]
	defaultBranch = main
[pull]
	rebase = false
[core]
	pager = delta
[interactive]
	diffFilter = delta --color-only
[delta]
	line-numbers = true
	diff-so-fancy = true
	file-style = omit
	hunk-header-style = omit
	side-by-side = false
	line-numbers-left-format = "{nm:>4} "
	line-numbers-right-format = "{np:>4} "
	line-numbers-left-style = dim
	line-numbers-right-style = dim
[merge]
	conflictstyle = zdiff3
